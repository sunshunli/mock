  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tianxiangbing/mock/issues"
  },
  "homepage": "https://github.com/tianxiangbing/mock#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "clipboard": "^2.0.0",
    "electron-packager": "^17.1.2",
    "express": "^4.16.2",
    "handlebars": "^4.0.11",
    "jq-dialog": "^2.1.2",
    "jq-query": "latest",
    "jquery": "^3.1.1",
    "rcedit": "^0.9.0",
    "socket.io": "^1.7.4",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "electron-winstaller": "^2.6.3"
